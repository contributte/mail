{
  "name": "contributte/mail",
  "description": "Extra contrib to nette/mail",
  "keywords": [
    "nette",
    "mail",
    "extra"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/mail",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nette/mail": "^3.1.0",
    "nette/utils": "^3.2.9 || ^4.0.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.5.1",
    "contributte/tester": "^0.2.0",
    "contributte/qa": "^0.4.0",
    "nette/di": "^3.1.2",
    "phpstan/phpstan": "^1.10.11",
    "phpstan/phpstan-deprecation-rules": "^1.1.3",
    "phpstan/phpstan-nette": "^1.2.4",
    "phpstan/phpstan-strict-rules": "^1.5.1",
    "tracy/tracy": "^2.10.2"
  },
  "suggest": {
    "contributte/di": "to use MailerExtension[CompilerExtension]"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Mail\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.7.x-dev"
    }
  }
}
